{
    "compilerOptions": {
        "target": "esnext",
        "module": "commonjs",
        "esModuleInterop": true,
        "skipLibCheck": true,
        "forceConsistentCasingInFileNames": true,
        "outDir": "./out",
        "resolveJsonModule": true,
        "sourceMap": true,
        "rootDir": "src", 
        "strict": true,
    },
    "exclude": ["node_modules", "out", "src/vue", "src/test", "ref", "third_party"]
}
